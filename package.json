{
  "name": "coordinape-app",
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": "=14"
  },
  "dependencies": {
    "@apollo/client": "3.5.7",
    "@craco/craco": "6.4.3",
    "@date-io/luxon": "1.3.13",
    "@gqty/react": "2.1.0",
    "@hcaptcha/react-hcaptcha": "0.3.7",
    "@material-ui/core": "4.11.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.58",
    "@material-ui/pickers": "3.3.10",
    "@prisma/client": "3.4.2",
    "@self.id/core": "0.1.0",
    "@sentry/integrations": "6.17.1",
    "@sentry/react": "6.17.1",
    "@sentry/tracing": "6.17.1",
    "@stitches/react": "1.2.6",
    "@web3-react/core": "6.1.9",
    "@web3-react/fortmatic-connector": "6.1.6",
    "@web3-react/injected-connector": "6.0.7",
    "@web3-react/portis-connector": "6.1.9",
    "@web3-react/walletconnect-connector": "6.2.4",
    "@web3-react/walletlink-connector": "6.2.5",
    "axios": "0.21.2",
    "clsx": "1.1.1",
    "d3-force-3d": "2.3.2",
    "ethereumjs-util": "7.1.3",
    "ethers": "5.5.1",
    "faker": "5.5.3",
    "figma-api": "^1.9.2",
    "gqty": "2.1.0",
    "graphql": "16.2.0",
    "graphql-zeus": "4.0.4",
    "itiriri": "2.0.1",
    "lodash": "4.17.21",
    "logdown": "3.3.1",
    "luxon": "2.0.2",
    "node-fetch": "2.6.1",
    "notistack": "1.0.10",
    "polished": "4.1.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-force-graph-2d": "1.23.8",
    "react-query": "3.34.8",
    "react-router-dom": "6.2.1",
    "react-string-replace": "0.4.4",
    "react-virtualized-auto-sizer": "1.0.5",
    "recoil": "0.4.1",
    "source-map-explorer": "2.5.2",
    "styled-components": "5.2.3",
    "true-myth": "4.1.1",
    "zod": "3.7.1"
  },
  "resolutions": {
    "canvas-color-tracker": "file:./canvas-color-tracker"
  },
  "scripts": {
    "setup": "./scripts/setup.sh",
    "db-seed-fresh": "yarn db-clean && yarn db-seed && yarn db-add-me",
    "db-seed": "node ./scripts/tsrun.mjs ./scripts/db-seed.ts",
    "db-clean": "node ./scripts/tsrun.mjs ./scripts/db-clean.ts",
    "db-add-me": "node ./scripts/tsrun.mjs ./scripts/db-add-me.ts",
    "generate": "prisma generate && ./scripts/generate-zeus.sh",
    "hasura": "hasura --envfile ../.env --project ./hasura",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose down -v",
    "start": "craco start",
    "build": "craco build",
    "build:local": "craco  --max_old_space_size=4096 build",
    "build:gzip": "craco build && gzipper --verbose ./build",
    "hardhat:compile": "yarn --cwd hardhat compile",
    "hardhat:codegen": "yarn --cwd hardhat codegen",
    "hardhat:build": "yarn --cwd hardhat build",
    "hardhat:dev": "yarn --cwd hardhat dev",
    "hardhat:deploy": "yarn --cwd hardhat deploy",
    "hardhat:install": "yarn --cwd hardhat install",
    "hardhat:test": "yarn --cwd hardhat test",
    "test": "craco test",
    "test:ci": "CI=1 craco test --coverage && yarn hardhat:test",
    "eject": "craco eject",
    "format:staged": "lint-staged",
    "lint": "eslint",
    "lint:check": "eslint \"{api,src}/**/*.{js,ts,tsx}\"",
    "lint:fix": "yarn lint:check --fix",
    "prettier": "prettier \"{api,src}/**/*.{js,ts,tsx}\"",
    "prettier:check": "yarn prettier --check",
    "prettier:fix": "yarn prettier --write",
    "typecheck": "tsc --noEmit",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "figma-colors": "node ./scripts/tsrun.mjs ./src/ui/scripts/figma-colors.ts"
  },
  "lint-staged": {
    "{api,src}/**/*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@datamodels/identity-accounts-crypto": "0.1.2",
    "@datamodels/identity-accounts-web": "0.1.2",
    "@datamodels/identity-profile-basic": "0.1.2",
    "@glazed/types": "0.1.3",
    "@gqty/cli": "2.3.1",
    "@sentry/webpack-plugin": "1.18.4",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/builder-webpack5": "^6.4.14",
    "@storybook/manager-webpack5": "^6.4.14",
    "@storybook/node-logger": "^6.4.14",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.14",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "12.1.2",
    "@types/debug": "4.1.7",
    "@types/ethereumjs-util": "6.1.0",
    "@types/faker": "^5.5.9",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.168",
    "@types/luxon": "2.0.2",
    "@types/node": "12.20.1",
    "@types/react": "17.0.27",
    "@types/react-dom": "17.0.9",
    "@types/react-router-dom": "5.1.7",
    "@types/react-virtualized-auto-sizer": "1.0.0",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "@vercel/node": "1.12.1",
    "assert": "2.0.0",
    "buffer": "6.0.3",
    "crypto-browserify": "3.12.0",
    "dotenv-expand": "6.0.1",
    "esbuild": "0.12.29",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "execa": "6.0.0",
    "http-browserify": "1.7.0",
    "https-browserify": "1.0.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "os-browserify": "0.3.0",
    "prettier": "2.4.1",
    "pretty-quick": "3.1.1",
    "prisma": "3.4.2",
    "react-scripts": "5.0.0-next.47",
    "source-map-explorer": "2.5.2",
    "storybook-addon-designs": "~6.2.1",
    "stream-browserify": "3.0.0",
    "tmp": "0.2.1",
    "ts-auto-mock": "3.5.0",
    "typescript": "4.4.3",
    "util": "0.12.4",
    "webpack": "5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ]
  }
}
